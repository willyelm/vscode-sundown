{
	"$schema": "vscode://schemas/color-theme",
	"name": "Dark (Sundown)",
	"include": "./dark-ui.json",
	"tokenColors": [
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#575757"
			}
		},
		{
			"name": "Foreground, Operator",
			"scope": [
				"keyword.operator.class",
				"keyword.operator",
				"constant.other",
				"source.php.embedded.line",
				"support.type.property-name.json",
				// "entity.name.function"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Variable, String Link, Regular Expression, Tag Name, GitGutter deleted",
			"scope": [
				"variable",
				"support.other.variable",
				"string.other.link",
				"string.regexp",
				"entity.other.attribute-name",
				"meta.tag",
				"variable.parameter",
				"declaration.tag",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#f2f2f2"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": "constant.numeric, constant.language, support.constant, constant.character, punctuation.section.embedded, keyword.other.unit",
			"settings": {
				"foreground": "#ebdf97"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name.class",
				"entity.name.type",
				"support.type",
				"support.class"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#d5bbfa"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading, GitGutter inserted",
			"scope": [
				"string",
				"constant.other.symbol",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#f19180"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": "keyword.operator, constant.other.color",
			"settings": {
				"foreground": "#caeff8",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Function, Special Method, Block Level, GitGutter changed",
			"scope": "meta.function-call, support.function, keyword.other.special-method, meta.block-level, markup.changed.git_gutter",
			"settings": {
				"fontStyle": "",
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage",
				"storage.type"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#f2f2f2"
			}
		},
		{
			"name": "Entities",
			"scope": [
				"entity.name.tag.css",
				"entity.name.tag"
			],
			"settings": {
				// "fontStyle": "bold",
				"foreground": "#ee81b0"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"fontStyle": "",
				"foreground": "#aeb3c2"
			}
		},
		{
			"name": "Separator",
			"scope": "meta.separator",
			"settings": {
				"foreground": "#858892"
			}
		},
		{
			"name": "Deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"fontStyle": "",
				"foreground": "#D2DDE2"
			}
		}
	],
	"semanticHighlighting": false,
	"semanticTokenColors": {
		"newOperator":"#C586C0",
		"stringLiteral":"#ce9178",
		"customLiteral": "#DCDCAA",
		"numberLiteral": "#b5cea8",
	}
}